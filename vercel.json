{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "public": false,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/queryOpenSources",
      "destination": "/api/queryOpenSources.js"
    }
  ]
}
